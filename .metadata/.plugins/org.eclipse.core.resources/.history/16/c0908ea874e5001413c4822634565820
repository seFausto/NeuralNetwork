
public class NeuralNetwork {

	public static void main(String[] args) {
		
		
		
		Neuron neuron = new Neuron();
		
		neuron.Train(1, 2);
		neuron.Train(2, 3);
		neuron.Train(3, 5);
		neuron.Train(4, 6);
		neuron.Train(5, 7);
		neuron.Train(6, 8);
		neuron.Train(7, 9);
		neuron.Train(8, 10);
		neuron.Train(9, 11);
		neuron.Train(0, 2);
		
		
		neuron.GetOuput(0);
		
		
		System.out.println("test");
	}
}
