
public class NeuralNetwork {

	public static void main(String[] args) {
		
		
		
		Neuron neuron = new Neuron();
		
		
		double[] inputs = new double[10];
		
		for (int i = 0; i < inputs.length; i++) {
			inputs[i] = i;
		}
		
		neuron.Train(inputs, 2);
		
		
		
		
		
		System.out.println(neuron.GetOuput(0));
	}
}
