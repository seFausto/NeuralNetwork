import java.util.Random;

public class TraningValues {

	public static void main(String[] args) {

		for (int i = 0; i < 100; i++) {
			System.out.println(Equation(GetNextX()));
		}

	}

	private static double GetNextX() {
		double randomX = 0;
		double upper = 3;
		double lower = .5;
		do {
			randomX = (Math.random() * (upper - lower)) + lower;
		} while (randomX >= .5 && randomX <= 3);
		return randomX;
	}

	public static double Equation(double x) {
		double result = 0.0;

		double xTo3 = Math.pow(x, 3);
		double xTo2 = Math.pow(x, 2);

		result = xTo3 - (2 * xTo2) - (2 * x) + 1;

		return result;

	}

}
